{
  "multipart": [
    {
      "when":  { "state": "active" },
      "apply": { "model": "hee:block/puzzle_base_active" }
    },
    {
      "when":  { "state": "inactive" },
      "apply": { "model": "hee:block/puzzle_base_inactive" }
    },
    {
      "when":  { "state": "disabled" },
      "apply": { "model": "hee:block/puzzle_base_disabled" }
    },
    {
      "when":  { "facing": "north" },
      "apply": { "model": "hee:block/puzzle_overlay_redirect_2ns" }
    },
    {
      "when":  { "facing": "east" },
      "apply": { "model": "hee:block/puzzle_overlay_redirect_2ew" }
    },
    {
      "when":  { "facing": "south" },
      "apply": { "model": "hee:block/puzzle_overlay_redirect_2ns" }
    },
    {
      "when":  { "facing": "west" },
      "apply": { "model": "hee:block/puzzle_overlay_redirect_2ew" }
    }
  ]
}
